        "def clean_data(dataframe):\n",
        "\n",
        "  # Eliminamos los duplicados  \n",
        "    dataframe.drop_duplicates(subset = \"names\", inplace=True)\n",
        "    dataframe = dataframe.reset_index(drop = True)\n",
        "\n",
        "    # Eliminamos las filas que contienen valores faltantes\n",
        "    dataframe = dataframe.replace('', pd.NA)\n",
        "    dataframe = dataframe.dropna()\n",
        "\n",
        "    # Eliminamos los que no corresponden a un nivel educacional  \n",
        "    dataframe = education(dataframe)\n",
        "    dataframe = dataframe.reset_index(drop = True)\n",
        "    \n",
        "    # Eliminamos los outliers\n",
        "    dataframe = quant_cut(dataframe)\n",
        "    dataframe = dataframe.reset_index(drop = True)\n",
        "    \n",
        "    \n",
        "    \n",
        "    \n",
        "    # Se resetean los indices \n",
        "    dataframe = dataframe.reset_index(drop = True)\n",
        "\n",
        "    return dataframe\n",
        "\n",
        "df_grades = pd.read_json(\"students_grades.json\")\n",
        "df_grades = clean_data(df_grades)\n",
        "df_grades"
