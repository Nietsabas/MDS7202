        "def quant_cut(dataframe):\n",
        "    dataframe[['math score', 'reading score', 'writing score']] = (\n",
        "     dataframe[['math score', 'reading score', 'writing score']].astype(int)\n",
        "    )\n",
        "    df_notas = dataframe[['math score', 'reading score', 'writing score']]\n",
        "\n",
        "    q1, q3 = (\n",
        "        df_notas[['math score', 'reading score', 'writing score']]\n",
        "        .quantile([0.25,0.75], numeric_only = True).values\n",
        "    )\n",
        "    iqr = q3 - q1 +1\n",
        "    upper_bound = q3 + 1.5 * iqr\n",
        "    lower_bound = q1 - 1.5 * iqr\n",
        "\n",
        "    scores = ((dataframe['math score'] >= lower_bound[0]) & \n",
        "              (dataframe['math score'] <= upper_bound[0]) &\n",
        "              (dataframe['reading score'] >= lower_bound[1]) & \n",
        "              (dataframe['reading score'] <= upper_bound[1]) &\n",
        "              (dataframe['writing score'] >= lower_bound[2]) & \n",
        "              (dataframe['writing score'] <= upper_bound[2]))\n",
        "    dataframe = dataframe.loc[scores]\n",
        "\n",
        "    return dataframe\n"
      ],
      "metadata": {
        "id": "F00v0_Bw3NBN"
      },
      "execution_count": 290,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def education(dataframe):\n",
        "  nivel_educativo = (\n",
        "  (dataframe['parental level of education'] == \"some high school\") |\n",
        "  (dataframe['parental level of education'] == \"high school\") |\n",
        "  (dataframe['parental level of education'] == \"some college\") |\n",
        "  (dataframe['parental level of education'] == \"associate's degree\") |\n",
        "  (dataframe['parental level of education'] == \"bachelor's degree\") |\n",
        "  (dataframe['parental level of education'] == \"master's degree\")\n",
        "  )\n",
        "  dataframe = dataframe.loc[nivel_educativo]\n",
        "\n",
        "  return dataframe\n",
        "\n"
      ],
      "metadata": {
        "id": "tTcYqlUE3jWG"
      },
      "execution_count": 291,
      "outputs": []
